<div id="tokenGeneratorView" class=".table-responsive" ng-controller="tokenController" ng-init="language != undefined">


    <div class="row">
        <div class="col-lg-12">
            <label>Generate Token</label>           
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <span>By default token expires after a day, going to add  setting later</span>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-lg-12">
            <div id="tokenGrid" dx-data-grid="tokenGridOptions"></div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-lg-10">
      
        </div>
        <div class="col-lg-2">
            <input type="submit" value="Generate Token" class="btn-sm btn-primary"   ng-click="generateToken()"/>
        </div>
    </div>
</div>